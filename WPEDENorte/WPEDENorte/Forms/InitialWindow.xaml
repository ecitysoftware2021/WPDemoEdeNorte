<Window x:Class="WPEDENorte.Forms.InitialWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPEDENorte.Forms"
        mc:Ignorable="d"
        Title="InitialWindow"  WindowStyle="None"
        WindowState="Maximized"
        Height="2064.095" 
        Width="1250.599"
        WindowStartupLocation="CenterScreen"
       >
    <Grid PreviewStylusDown="Grid_PreviewStylusDown"
          PreviewMouseLeftButtonUp="Grid_PreviewMouseLeftButtonUp"  >
        <Grid.Background>
            <ImageBrush ImageSource="{Binding sourse}"/>
        </Grid.Background>
    </Grid>
</Window>
